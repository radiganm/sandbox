#!/usr/bin/open-axiom --no-hyperdoc --script
++ archimedes-pi-approximation-quad-double.input
++ Copyright 2016 Mac Radigan
++ All Rights Reserved

  e2k(e2)    == 2 - 2 * (1 - e2/4)**(1/2)
  sk(s)      == 2 * s

  k  := 16 :: Integer
  e2 := 2  :: Float
  s  := 2  :: Float
  repeat
    if k < 1 then break
    e2 := e2k(e2)
    s  := sk(s)
    k  := k - 1

  api := s * e2**(1/2)
  err := %pi::Float - api
  print api
  print err

++ *EOF*
